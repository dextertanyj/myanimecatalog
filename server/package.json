{
  "name": "myanimecatalog-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Dexter Tan",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nodemon -e ts,graphql -x \"eslint --ext .ts . && ts-node --files src/index.ts\"",
    "manualStart": "nodemon -r dotenv/config ./src/index.ts",
    "migration:gen": "prisma migrate dev --create-only",
    "migration:run": "prisma migrate dev",
    "migration:deploy": "docker exec server prisma migrate deploy",
    "server:sh": "docker exec -it $(docker-compose ps -q server) sh",
    "lint": "eslint --fix \"src/**/*.ts\""
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.14",
    "@graphql-tools/load": "^7.5.13",
    "@prisma/client": "^3.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.22",
    "@types/sha.js": "^2.4.0",
    "apollo-server": "^3.8.2",
    "apollo-server-core": "^3.8.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "graphql": "15.7.2",
    "graphql-middleware": "^6.1.28",
    "graphql-shield": "^7.5.0",
    "graphql-tools": "^8.2.11",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.2",
    "sha.js": "^2.4.11",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@prisma/codemods": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "ts-node": "^10.8.1"
  }
